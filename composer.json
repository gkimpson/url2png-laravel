{
    "name": "gkimpson/url2png-laravel",
    "description": "Laravel wrapper for url2png API for generating website thumbnails, an api key & secret key is required from https://www.url2png.com/",
    "keywords": [
        "gkimpson",
        "url2png-laravel"
    ],
    "homepage": "https://github.com/gkimpson/url2png-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Gavin Kimpson",
            "email": "gkimpson@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Gkimpson\\Url2pngLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gkimpson\\Url2pngLaravel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "0.0.3"
}
